<%= turbo_stream_from "presentations" %>

<div
  class='row'
  style="background-image: url('<%= asset_path("Fundo_1.png") %>'); background-size: cover; background-position: center;"
  data-controller='fullscreen'
  data-action='click->fullscreen#enterFullscreen touchstart->fullscreen#enterFullscreen'
>
  <div style="height: 14vh;">
    <%= render "header" %>
  </div>

  <div>
    <%= render "presentations_table", presentations_by_room: @presentations_by_room %>
  </div>
</div>
